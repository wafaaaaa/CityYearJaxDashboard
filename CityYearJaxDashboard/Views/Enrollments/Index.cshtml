@model IEnumerable<CityYearJaxDashboard.Views.Shared.Enrollment>

@{
    ViewBag.Title = "Index";
}
<html>
<head>
    <style>
        body {
            margin: 0;
        }
        ul.nav navbar-nav{
            background-color: #000000;
        }
        ul.list,h4 {
            list-style-type: none;  
            margin: 0;
            padding: 0;
            width: 35%;
            background-color: #f1f1f1;
            position: static;
            height: 70%;
            overflow: auto;
        }
    </style>
</head>
<body>
    <div class="container2" style="margin-left:21%"
    <h2>Service Delivery</h2><br />
    <ul class="list">
        @* <li><a class="active" href="#home">Home</a></li>
            <li><a href="#news">News</a></li>
            <li><a href="#contact">Contact</a></li>
            <li><a href="#about">About</a></li>*@
        <h4>Category</h4>
        <li><input type="checkbox" id="ela" value="ela"> ELA </li>
        <li><input type="checkbox" id="math" value="math"> Math</li>
        <li><input type="checkbox" id="attendance" value="attendance"> Attendance </li>
        <li><input type="checkbox" id="behavior" value="behavior"> Behavior</li>
        <br />
        <h4>Subcategory</h4>
        <li><input type="checkbox" id="enrollment" value="enrollment"> Enrollment</li>
        <li><input type="checkbox" id="dosage" value="dosage"> Dosage</li>
        <li><input type="checkbox" id="prevention" value="prevention"> Prevention </li>
        <li><input type="checkbox" id="growth" value="growth"> Growth</li>
        <li><input type="checkbox" id="recovery" value="recovery"> Recovery</li>
        <br />
        <h4>Filter Goals</h4>
        <li><input type="checkbox" id="q1" value="q1"> Q1</li>
        <li><input type="checkbox" id="q1actual" value="q1actual"> Q1 Actual</li>
        <li><input type="checkbox" id="q2" value="q2"> Q2 </li>
        <li><input type="checkbox" id="q2actual" value="q2actual"> Q2 Actual</li>
        <li><input type="checkbox" id="q3" value="q3"> Q3 </li>
        <li><input type="checkbox" id="q3actual" value="q3actual"> Q3 Actual</li>
        <li><input type="checkbox" id="q4" value="q4">  Q4  </li>
        <li><input type="checkbox" id="q4actual" value="q4actual"> Q4 Actual</li>
        <li><input type="checkbox" id="bestofcy" value="bestofcy"> Best of City Year </li>
        <li><input type="checkbox" id="annual" value="annual"> Annual </li>
    </ul>
    <br />
    <input type="reset" />
    <input type="button" value="Generate" onclick="" />
    <hr />
    <h2>Goals</h2>
    <p>
        @Html.ActionLink("Create New", "Enrollments")
    </p>
    <table class="table">
        <tr>
            <th>
                Q1 Goal
            </th>
            <th>
                Q1 Actual
            </th>
            <th>
                Q2 Goal
            </th>
            <th>
                Q2 Actual
            </th>
            <th>
                Q3 Goal
            </th>
            <th>
                Q3 Actual
            </th>
            <th>
                Q4 Goal
            </th>
            <th>
                Q4 Actual
            </th>
            <th>
                Best of CY Goal
            </th>
            <th>
                Annual Goal
            </th>

            <th></th>
        </tr>

       @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Q1Goal)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Q1Actual)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Q2Goal)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Q2Actual)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Q3Goal)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Q3Actual)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Q4Goal)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Q4Actual)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AnnualGoal)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BestofCYGoal)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Enrollment_ID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Enrollment_ID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Enrollment_ID })
                </td>
            </tr>
        }
          
    </table>
    </div>
</body>
</html>

